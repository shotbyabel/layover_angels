

 <div class="row">
  <div class="col-md-3 col-md-offset-4">
 <h3>Login to Layover Angels</h3>
  <%= form_tag sessions_path do %>
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="actions"><%= submit_tag "Log in" %></div>

    

  <% end %>





